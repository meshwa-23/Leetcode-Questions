class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        int n = nums.length;
        int maxCount = 0;
        int counter = 0;
        for(int i = 0;i<n;i++){
            if(nums[i]== 1){
            counter++;
            maxCount  = Math.max(maxCount,counter);
            }

           else{
            counter = 0;
           }  
        } 
        return maxCount;
    
    }
}
