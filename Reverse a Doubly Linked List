class Solution {
    public Node reverse(Node head) {
        // code here
        if(head == null && head.next == null ){
            return head;
        }
        Node prev = null;
        Node current = head;
        while(current != null){
            prev = current.prev;
            current .prev = current . next;
            current.next = prev;
        }
        return prev.prev;
    }
}
